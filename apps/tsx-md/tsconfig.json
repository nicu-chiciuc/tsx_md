{
  "exclude": ["node_modules"],
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "outDir": "dist",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "esModuleInterop": true,
    "strictNullChecks": true
  },
  "include": ["src", "next-env.d.ts", ".next/types/**/*.ts", "pages", "app", "lib"]
}
